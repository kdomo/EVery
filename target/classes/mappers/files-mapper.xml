<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="filesMapper">
	<insert id="insertFile" parameterType="FilesDTO">
		insert into files values(seq_file.nextval,#{seq_board},#{ori_name},#{sys_name})
	</insert>

	<select id="selectByNo" resultType="FilesDTO">
		select * from files where seq_board = #{seq_board}
	</select>
</mapper>